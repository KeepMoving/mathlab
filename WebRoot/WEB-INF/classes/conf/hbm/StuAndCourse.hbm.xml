<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping 	package="com.mathlab.model">
	<class name="StuAndCourse" table="stu2course">
		<id name="ID" column="ID">
			<generator class="identity"/>
		</id>				
		<property name="stuNum" column="stu2course_stuNum" type="string"/>
		<property name="courseID" column="stu2course_courseID"/>		
	</class>
</hibernate-mapping>
