<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping 	package="com.mathlab.model">
	<class name="Note" table="note">
				<id name="id" 
			column="id">
			<generator class="identity"/>
		</id>				
		<property name="userName" column="note_username" type="string"/>
		<property name="note" column="note_note" type="string"/>
		<property name="date" column="note_date" type="string"/>				
	</class>
</hibernate-mapping>
