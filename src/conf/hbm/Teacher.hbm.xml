<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping 	package="com.mathlab.model">
	<class name="Teacher" table="tea">
		<id name="teaID" column="teaID">
			<generator class="identity"/>
		</id>				
		<property name="teaName" column="tea_teaName" type="string"/>
		<property name="teaPwd" column="tea_teaPwd" type="string"/>
		<property name="authority" column="tea_authority" type="string"/>
	</class>
</hibernate-mapping>
